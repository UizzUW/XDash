<?xml version="1.0" encoding="utf-8" ?>
<mvp:PathwayApplication xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XDash.App"
             xmlns:mvp="clr-namespace:MVPathway;assembly=MVPathway"
             xmlns:xdashConverters="clr-namespace:XDash.Converters;assembly=XDash">
    <mvp:PathwayApplication.Resources>
        <ResourceDictionary>
            <xdashConverters:LanguageEnumToNameConverter x:Key="LanguageEnumToNameConverter"/>
            <xdashConverters:OperatingSystemToLogoConverter x:Key="OperatingSystemToLogoConverter" />
        </ResourceDictionary>
    </mvp:PathwayApplication.Resources>
</mvp:PathwayApplication>